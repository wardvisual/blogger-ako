model Blogger {
    id       Int    @id @default(autoincrement())
    name     String
    usernmae String
}

model Wow {
    id       Int    @id @default(autoincrement())
    name     String
    usernmae String
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model haha {
    id       Int    @id @default(autoincrement())
    name     String
    usernmae String
}
